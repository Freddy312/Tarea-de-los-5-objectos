	money: 89,
	id: 920071195,
	edad: 20,
	fullName: function() {return this.firstName + " " + this.lastName;},
	canBuy: function(n) {return this.money >= n;} 
};

var carro = {
    marca: "Delorian",
	costo: 500000,
	placas: "P23-625T-4M",
	color: "Gris",
	puertas inclinadas: true,
	nuevasPlacas: function(str) { this.placas = str; },
	alternarFocos: function() { this.puertasInclinadas = !this.puertasInclinadas; }
};

var Invocador = {
	nick: "MrMattews",
	ip: "85.12.34.223",
	money: 345,
	homeID: 765,
	ticks: 14,
	newNick: function(s) { this.nick = s; },
	disconnectPlayer: function() { this.ip = ""; }
};

var Minecraft= {
    players: 0,
	maxPlayers: 5,
	mapID: 257,
	serverName: "Utopia",
	hash: 154,
	addPlayer: function() { players++; },
	overpopulated: function() { return players > maxPlayers; }
};

